{
  "name": "fv-ai-wasm",
  "version": "1.0.0",
  "description": "Bot AI for Fish Volleyball.",
  "author": "Ashton Meuser",
  "license": "MIT",
  "devDependencies": {
    "@assemblyscript/wasi-shim": "^0.1.0",
    "assemblyscript": "^0.27.5"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  },
  "scripts": {
    "build": "asc assembly/$npm_config_file.ts --target release --outFile build/$npm_config_file.wasm",
    "build:all": "npm run build:simple && npm run build:import && npm run build:wasi",
    "build:simple": "npm run build --file=simple",
    "build:import": "npm run build --file=import",
    "build:wasi": "npm run build --file=wasi"
  }
}
